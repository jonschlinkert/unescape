{
  "name": "unescape",
  "description": "Convert HTML entities to HTML characters, e.g. `&gt;` converts to `>`.",
  "version": "1.2.1",
  "homepage": "https://github.com/jonschlinkert/unescape",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/unescape",
  "bugs": {
    "url": "https://github.com/jonschlinkert/unescape/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "gulp-format-md": "^1.0.0",
    "mocha": "^5.2.0"
  },
  "keywords": [
    "char",
    "character",
    "characters",
    "entities",
    "entity",
    "escape",
    "html",
    "string",
    "un-escape",
    "unescape",
    "xml"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": [
        "html-elements",
        "html-tag",
        "html-toc",
        "is-self-closing"
      ]
    },
    "reflinks": [
      "ent",
      "html-entities"
    ]
  }
}
